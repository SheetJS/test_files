 <Worksheet ss:Name="MortgageCalculator">
  <Table ss:ExpandedColumnCount="2" ss:ExpandedRowCount="4" x:FullColumns="1"
   x:FullRows="1" ss:DefaultColumnWidth="53" ss:DefaultRowHeight="14">
   <Row>
    <Cell><Data ss:Type="String">Principal</Data></Cell>
    <Cell><Data ss:Type="Number">100000</Data></Cell>
   </Row>
